<form bindsubmit="formSubmit">
  <view class="box">
    <text>姓名</text>
    <input name="stuName" focus="true" placeholder="请填写姓名" />
  </view>
  <view class="box">
    <text>性别</text>
    <radio-group name="gender">
      <label style="margin-right:20rpx;">
        <radio value="1" />男
      </label>
      <label>
        <radio value="2" />女
      </label>
    </radio-group>
  </view>
  <view class="box">
    <text>学号</text>
    <input name="stuNo" focus="true" placeholder="请填写学号" />
  </view>

  <view class="box">
    <text>身份证后六位</text>
    <input name="lastSixDigitsOfIdCard" focus="true" placeholder="请填写身份证后六位" />
  </view>

  <view class="box">
    <text>手机</text>
    <input name="phone" focus="true" placeholder="请填写手机号" />
  </view>

  <view class="box">
    <text>邮箱</text>
    <input name="email" focus="true" placeholder="请填写邮箱" />
  </view>

  <view class="box">
    <text>地址</text>
    <input name="address" focus="true" placeholder="请填写家庭地址" />
  </view>

  <!-- <view class="button" bindtap="sub">提交</view> -->
  <button formType="submit">1111</button>
</form>
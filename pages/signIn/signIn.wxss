/* pages/clockIn.wxss */

map {
  height: 100vh;
  width: 100%;
}

.clock{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background-color: white;
  border: 14rpx solid #3179F7;
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  margin-left: -100rpx;
  text-align: center;
  font-size: 44rpx;
  line-height: 200rpx;
}


